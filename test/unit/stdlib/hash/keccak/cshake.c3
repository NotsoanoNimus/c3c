fn void cshake_empty()
{
	String[][] expected = {
		{
			x"",
			x"",
			x"",
		},
		{
			x"",
			x"",
			x"",
		}
	};

	test::eq(cshake::xof(128, 256, "", tests_common::CUSTOMIZATION_STRINGS[0])[..], expected[0][0][..]);
	test::eq(cshake::xof(128, 256, "", tests_common::CUSTOMIZATION_STRINGS[1])[..], expected[0][1][..]);
	test::eq(cshake::xof(128, 256, "", tests_common::CUSTOMIZATION_STRINGS[2])[..], expected[0][2][..]);

	test::eq(cshake::xof(256, 256, "", tests_common::CUSTOMIZATION_STRINGS[0])[..], expected[1][0][..]);
	test::eq(cshake::xof(256, 256, "", tests_common::CUSTOMIZATION_STRINGS[1])[..], expected[1][1][..]);
	test::eq(cshake::xof(256, 256, "", tests_common::CUSTOMIZATION_STRINGS[2])[..], expected[1][2][..]);
}

