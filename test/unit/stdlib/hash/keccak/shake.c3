	test::eq(x"25904657e9903ce960b56bcc42a4e9ff7b33", shake::hash_128(18, x"ca12721a7a44544d9518aa0d4e407529"));
	test::eq(x"3b37b980afb04ada89695c70c27eb887a913a502ed", shake::hash_128(21, x"81c8bfe6fc0b1e92ddfad4215752bdd2"));
	test::eq(x"b824c8f435b8f86863d40522bc5875be67bab4f798794c7d", shake::hash_128(24, x"382ce6a97be09193404b5f3865e6baeb"));
	test::eq(x"6136ccea5eb15b2229d6fc169db69d229fd0fe7a02ed6d87ea58", shake::hash_128(26, x"d87f23db7553ebfceaaee817a49b0f1c"));
	test::eq(shake::xof(128, 32, x"e01cbb1ab95410653be263b1c63e9e53"), x"4abd4c46f3d06e4fd8e0e7b0a98496159350ce8af7ea331772895d71ecf8af63");
	test::eq(shake::xof(128, 45, x"0969c08355404ee5502d29aa38081d84"), x"2bf4b5d1027f64b9715d563ad08bb325c135192e4702189b3a42683bead14e69121038852796fba5eae3426d33");

	test::eq(x"23ce", shake::hash_256(2, x"c61a9188812ae73994bc0d6d4021e31bf124dc72669749111232da7ac29e61c4"));
	test::eq(x"ae53c844d511", shake::hash_256(6, x"dca55c76165b528f2080566538adb1f0053b66f3cea80b82ea3bd8c278ebfef0"));
	test::eq(x"3389aea66244b91428", shake::hash_256(9, x"3e20cf32669fa3fd6e94e519b52a1dba33cd1f3a6947975e9829e4db326d2a18"));
	test::eq(x"7314002948c057006d4fc21e3e19c258", shake::hash_256(16, x"e3ef127eadfafaf40408cebb28705df30b68d99dfa1893507ef3062d85461715"));
	test::eq(x"e7708cdc22f03b0bfaca03e5d11d46cac118", shake::hash_256(18, x"445b17ce13727ae842b877c4750611a9eb79823bc5752da0a5e9d4e27bd40b94"));
	test::eq(shake::xof(256, 59, x"7d9312ffe94845ac51056c63eb3bff4a94626aafb7470ff86fa88fd8f0fe45c9"), x"de489392796fd3b530c506e482936afcfe6b72dcf7e9def054953842ff19076908c8a1d6a4e7639e0fdbfa1b5201095051aac3e3997779e588377e");
	test::eq(shake::xof(256, 250, x"445b17ce13727ae842b877c4750611a9eb79823bc5752da0a5e9d4e27bd40b94"), x"");
}

